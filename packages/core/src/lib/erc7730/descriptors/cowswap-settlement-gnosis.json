{
  "$schema": "https://github.com/LedgerHQ/clear-signing-erc7730-registry/blob/main/specs/erc7730-v1.schema.json",
  "context": {
    "contract": {
      "deployments": [
        {
          "chainId": 100,
          "address": "0x9008D19f58AAbD9eD0D60971565AA8510560ab41"
        }
      ]
    }
  },
  "metadata": {
    "owner": "CoW Protocol",
    "info": {
      "url": "https://cow.fi",
      "legalName": "CoW DAO"
    },
    "enums": {
      "OrderAction": {
        "true": "Create Order",
        "false": "Cancel Order"
      }
    }
  },
  "display": {
    "formats": {
      "setPreSignature(bytes,bool)": {
        "intent": "Create or revoke CoW Swap pre-signed order",
        "fields": [
          {
            "label": "Action",
            "path": "#.signed",
            "format": "enum",
            "enum": "OrderAction"
          },
          {
            "label": "Order Hash",
            "path": "#.orderUid",
            "format": "raw"
          }
        ]
      },
      "invalidateOrder(bytes)": {
        "intent": "Cancel CoW Swap order",
        "fields": [
          {
            "label": "Order Hash",
            "path": "#.orderUid",
            "format": "raw"
          }
        ]
      }
    }
  }
}
